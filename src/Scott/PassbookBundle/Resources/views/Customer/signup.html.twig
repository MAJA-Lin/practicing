{% extends 'ScottPassbookBundle:Default:base.html.twig' %}

{% block body %}
    <h1>Sign up successfully!</h1>
    <br>
    {% set var = result | json_decode_array %}
    <h2>Your Email: {{ var.data.customer.email }}</h2>
    <h2>Currency: {{ var.data.account.currency }}</h2>
    <h2>Balance: {{ var.data.account.balance }}</h2>
    <br>
    <a href="{{ path('login') }}"><h3>Click here to Log in</h3></a>
{% endblock %}