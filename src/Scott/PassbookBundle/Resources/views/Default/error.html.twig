{% extends 'ScottPassbookBundle:Default:base.html.twig' %}

{% block body %}
    {% set var = result | json_decode_array %}
    {% if var.data.error.message == "Something went wrong! Please login again!" %}
         <script>alert('{{ var.data.error.message }}');
                window.location.href="{{ path('logout') }}";</script>
    {% endif %}

    <script>alert('{{ var.data.error.message }}');
        window.location.href=history.back();</script>
{% endblock %}